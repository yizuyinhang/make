.view{
  width: 84%;
  height: auto;
  margin-left: 5%;
  margin-bottom: 20rpx;
  padding: 0rpx 25rpx;
  border: 1rpx solid #F1F1F1;
  border-radius: 10rpx;
}
.body{
  width: 100%;
  height: 40rpx;
  padding: 30rpx 0rpx;
  font-size: 28rpx;
  border-top: solid 1rpx #F1F1F1;
}
.title{
  float: left;
  color: #AAAAAA;
  width: auto;
  height: auto;
  margin-right: 20rpx;
}
.item{
  float: left;
  width: auto;
  height: auto;
  color: #333333;
}
.user{
  width: 100%;
  height: 80rpx;
  padding: 30rpx 0rpx;
  font-size: 28rpx;
  line-height: 80rpx;
}
.image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 10rpx;
  float: left;
  margin-right: 20rpx;
}
.doctor{
  float: left;
  font-size: 30rpx;
  margin-right: 20rpx;
}
.doctorclass{
  color: #555555;
  float: left;
  font-size: 24rpx;
}
.price{
  color: #f46363fc;
}
.button{
  color: #006EFF;
  font-size: 34rpx;
  background-color: #E4F0FF;
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 10rpx;
  margin-top: 30rpx;
  margin-left: 5%;
}
.top{
  width: 100%;
  height: 200rpx;
  text-align: center;
  margin-top: 20rpx;
}
.successimg{
  width: 92rpx;
  height: 92rpx;
}
.success{
color: 34rpx;
}